<Window x:Class="SpeedyPass.ViewAddPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpeedyPass"
        mc:Ignorable="d"
        Title="ViewAddPassword" Height="276.429" Width="284.643" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Domain" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox Height="23" Margin="10,36,9.6,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="{Binding PasswordDataModel.Domain, Mode=TwoWay}"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="10,68,0,0" VerticalAlignment="Top"/>
        <TextBox Height="23" Margin="10,94,9.6,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="{Binding PasswordDataModel.Username, Mode=TwoWay}"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="10,129,0,0" VerticalAlignment="Top"/>
        <TextBox Height="23" Margin="10,155,9.6,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="{Binding PasswordDataModel.Password, Mode=TwoWay}"/>
        <Button Content="Add" HorizontalAlignment="Right" Margin="0,199,9.6,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
